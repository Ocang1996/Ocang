{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:open": "vite --open",
    "dev:watch": "vite --open",
    "auto-refresh": "node auto-refresh.js",
    "dev:auto": "concurrently \"npm run dev:open\" \"npm run auto-refresh\"",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "setup:env": "node -e \"const fs = require('fs'); if (!fs.existsSync('.env')) { fs.copyFileSync('.env.example', '.env'); console.log('Created .env file from .env.example'); }\"",
    "predeploy": "npm run setup:env",
    "setup:supabase": "node setup-supabase.js"
  },
  "dependencies": {
    "@react-three/drei": "^9.99.0",
    "@react-three/fiber": "^8.15.19",
    "@supabase/supabase-js": "^2.49.4",
    "chart.js": "^4.4.2",
    "chartjs-plugin-datalabels": "^2.2.0",
    "clsx": "^2.1.0",
    "date-fns": "^3.6.0",
    "file-saver": "^2.0.5",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.344.0",
    "pptxgenjs": "^3.12.0",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.2.0",
    "react-datepicker": "^8.3.0",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.22.2",
    "react-to-print": "^3.1.0",
    "recharts": "^2.15.3",
    "tailwind-merge": "^2.2.1",
    "three": "^0.176.0",
    "uuid": "^11.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/file-saver": "^2.0.7",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "chokidar": "^4.0.3",
    "concurrently": "^9.1.2",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
